{
  "name": "template-genesis-theme",
  "version": "1.0.0",
  "description": "A template child theme for the Genesis framework for WordPress.",
  "private": true,
  "main": "js/main.js",
  "keywords": [
    "wordpress"
  ],
  "author": "Zachary K. Watkins <zwatkins.it@gmail.com>",
	"license": "GPL-2.0-or-later",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "watch": "npm run build:dev -- --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier": "prettier"
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.19.1",
    "core-js": "^3.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jsdoc": "^3.6.11",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "**/*.scss": [
      "prettier --write",
      "stylelint --report-needless-disables --report-invalid-scope-disables --allow-empty-input",
      "git add"
    ],
    "!(*sass).md": [
      "prettier --write",
      "git add"
    ]
  }
}
